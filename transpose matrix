#include <stdio.h>
#include <string.h>

void main() {
    int i,j,n,m;//N-number of matrix
    int mat1[50][50];
    printf("Enter the value of n: ");
    scanf("%d",&n);
    printf("Enter the value of m: ");
    scanf("%d",&m);
    printf("Matrix input: \n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            printf("element [%d][%d]: ",i,j);
            scanf("%d",&mat1[i][j]);
        }
    }
    
    printf("Transpose:\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d ",mat1[j][i]);
        }
        printf("\n");
    }
   
    
}
