#include <stdio.h>
int swap(int* n,int* m){
    int temp;
    temp=*n;
    *n=*m;
    *m=temp;
    return 0;
}
int main() {
    int a,b;
    printf("enter the two numbers: ");
    scanf("%d %d",&a,&b);
    printf("Before swapping: %d %d",a,b);
    swap(&a,&b);
    printf("\nAfter swapping: %d %d",a,b);
    return 0;
}
