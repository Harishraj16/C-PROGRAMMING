#include <stdio.h>

int main() {
    int arr[50],n,sum=0;
    printf("Enter the number of integers: ");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        printf("Enter the number %d: ",i+1);
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++){
        if(arr[i]%2!=0)
        sum+=arr[i];
    }
    printf("Sum: %d",sum);
}
